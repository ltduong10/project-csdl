<body>
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
   <%include headerarea%>
    <!-- Main Menu area start-->
    <div class="main-menu-area mg-tb-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        <li class="active"><a data-toggle="tab" href="#checkInfo"><i class="notika-icon notika-windows"></i> Xem thông tin</a>
                        </li>
                        <li><a data-toggle="tab" href="#addInfo"><i class="notika-icon notika-form"></i> Thêm thông tin</a>
                        </li>
                    </ul>
                    <div class="tab-content custom-menu-content">
                        <div id="checkInfo" class="tab-pane notika-tab-menu-bg in active animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li><a href="/admin/orderlist">Đơn hàng</a>
                                </li>
                                <li><a href="/admin/customers">Khách hàng</a>
                                </li>
                                <li><a href="/admin/productlist">Sản phẩm</a>
                                </li>
                                <li><a href="/admin/categorylist">Danh mục</a>
                                </li>
                                <li><a href="/admin/incategory">Sản phẩm thuộc danh mục</a>
                                </li>
                            </ul>
                        </div>
                        <div id="addInfo" class="tab-pane notika-tab-menu-bg animated flipInX">
                                <ul class="notika-main-menu-dropdown">
                                    <li><a href="/admin/addinfoproduct">Sản phẩm</a>
                                    </li>
                                    <li><a href="/admin/addtag">Tag</a>
                                    </li>
                                </ul>
                          </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Menu area End-->
    <!-- Data Table area Start-->
    <div class="data-table-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="data-table-list">
                        <div class="basic-tb-hd">
                            <h2>Danh sách đơn hàng</h2>
                        </div>
                        <div class="table-responsive">
                            <table id="data-table-basic" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>ID Đơn</th>
                                        <th>ID Khách</th>
                                        <th>Username</th>
                                        <th>Trạng thái</th>
                                        <th>Ngày đặt hàng</th>
                                        <th>Thêm</th>
                                        <th>Check</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% result.rows.forEach((p)=>{%>
                                        <tr>
                                            <td><%=p.orderid%></td>
                                            <td><%=p.customerid%></td>
                                            <td><%=p.username%></td>
                                            <td><%=p.orderstatus%></td>
                                            <td><%=moment(p.orderdate).format('MMMM Do YYYY, h:mm:ss a')%></td>
                                            <td><a href="/admin/orderdetails?id=<%=p.orderid%>">Chi tiết</a></td>
                                            <% if(p.orderstatus =='new'){%>
                                            <td><button style="" class="btn btn-default btn-icon-notika" onclick="window.location.href='/admin/confirmorder?id=<%=p.orderid%>'"><i class="notika-icon notika-checked"></i></button> <button class="btn btn-default btn-icon-notika" onclick="window.location.href='/admin/cancelorder?id=<%=p.orderid%>'"><i class="notika-icon notika-close"></i></button></td>
                                            <%}else {%>
                                                <td></td>
                                                <%}%>
                                        </tr>
                                    <%})%>
                                    
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>ID Đơn</th>
                                        <th>ID Khách</th>
                                        <th>Username</th>
                                        <th>Trạng thái</th>
                                        <th>Ngày đặt hàng</th>
                                        <th>Thêm</th>
                                        <th>Check</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Data Table area End-->
    <!-- Start Footer area-->
    <%include footer%>
    <!-- End Footer area-->
    <!-- jquery
		============================================ -->
    <script src="js/vendor/jquery-1.12.4.min.js"></script>
    <!-- bootstrap JS
		============================================ -->
